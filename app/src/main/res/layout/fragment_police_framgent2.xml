                android:layout_height="wrap_content"
                android:gravity="center"
                android:scrollbars="none"
                android:entries="@array/policefrg2_month"
                />
            <!--<TextView-->
            <!--android:layout_width="30dp"-->
            <!--android:layout_height="wrap_content"-->
            <!--android:layout_gravity="center_vertical"-->
            <!--android:text="月"-->
